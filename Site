function SnakeGame({ setShowSnakeGame, setActiveSection }) {
Â  const GRID_SIZE = 20;
Â  const CELL_SIZE = 20;
Â  const INITIAL_SNAKE = [{x: 10, y: 10}];
Â  const INITIAL_DIRECTION = {x: 1, y: 0};
Â  const TARGET_LENGTH = 10;

Â  const [snake, setSnake] = useState(INITIAL_SNAKE);
Â  const [direction, setDirection] = useState(INITIAL_DIRECTION);
Â  const [food, setFood] = useState({x: 15, y: 15});
Â  const [gameOver, setGameOver] = useState(false);
Â  const [won, setWon] = useState(false);

Â  const generateFood = useCallback(() => {
Â  Â  let newFood;
Â  Â  do {
Â  Â  Â  newFood = {
Â  Â  Â  Â  x: Math.floor(Math.random() * GRID_SIZE),
Â  Â  Â  Â  y: Math.floor(Math.random() * GRID_SIZE)
Â  Â  Â  };
Â  Â  } while (snake.some(segment => segment.x === newFood.x && segment.y === newFood.y));
Â  Â  return newFood;
Â  }, [snake]);

Â  useEffect(() => {
Â  Â  const handleKeyPress = (e) => {
Â  Â  Â  if (gameOver || won) return;
Â  Â  Â Â 
Â  Â  Â  switch(e.key) {
Â  Â  Â  Â  case 'ArrowUp':
Â  Â  Â  Â  Â  if (direction.y === 0) setDirection({x: 0, y: -1});
Â  Â  Â  Â  Â  break;
Â  Â  Â  Â  case 'ArrowDown':
Â  Â  Â  Â  Â  if (direction.y === 0) setDirection({x: 0, y: 1});
Â  Â  Â  Â  Â  break;
Â  Â  Â  Â  case 'ArrowLeft':
Â  Â  Â  Â  Â  if (direction.x === 0) setDirection({x: -1, y: 0});
Â  Â  Â  Â  Â  break;
Â  Â  Â  Â  case 'ArrowRight':
Â  Â  Â  Â  Â  if (direction.x === 0) setDirection({x: 1, y: 0});
Â  Â  Â  Â  Â  break;
Â  Â  Â  Â  default:
Â  Â  Â  Â  Â  break;
Â  Â  Â  }
Â  Â  };

Â  Â  window.addEventListener('keydown', handleKeyPress);
Â  Â  return () => window.removeEventListener('keydown', handleKeyPress);
Â  }, [direction, gameOver, won]);

Â  useEffect(() => {
Â  Â  if (gameOver || won) return;

Â  Â  const gameLoop = setInterval(() => {
Â  Â  Â  setSnake(prevSnake => {
Â  Â  Â  Â  const head = prevSnake[0];
Â  Â  Â  Â  const newHead = {
Â  Â  Â  Â  Â  x: (head.x + direction.x + GRID_SIZE) % GRID_SIZE,
Â  Â  Â  Â  Â  y: (head.y + direction.y + GRID_SIZE) % GRID_SIZE
Â  Â  Â  Â  };

Â  Â  Â  Â  // Check collision with self
Â  Â  Â  Â  if (prevSnake.some(segment => segment.x === newHead.x && segment.y === newHead.y)) {
Â  Â  Â  Â  Â  setGameOver(true);
Â  Â  Â  Â  Â  return prevSnake;
Â  Â  Â  Â  }

Â  Â  Â  Â  const newSnake = [newHead, ...prevSnake];

Â  Â  Â  Â  // Check if food is eaten
Â  Â  Â  Â  if (newHead.x === food.x && newHead.y === food.y) {
Â  Â  Â  Â  Â  if (newSnake.length >= TARGET_LENGTH) {
Â  Â  Â  Â  Â  Â  setWon(true);
Â  Â  Â  Â  Â  Â  return newSnake;
Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  setFood(generateFood());
Â  Â  Â  Â  } else {
Â  Â  Â  Â  Â  newSnake.pop();
Â  Â  Â  Â  }

Â  Â  Â  Â  return newSnake;
Â  Â  Â  });
Â  Â  }, 150);

Â  Â  return () => clearInterval(gameLoop);
Â  }, [direction, food, gameOver, won, generateFood]);

Â  const handleWinContinue = () => {
Â  Â  setShowSnakeGame(false);
Â  Â  setActiveSection('search');
Â  };

Â  const handleRestart = () => {
Â  Â  setSnake(INITIAL_SNAKE);
Â  Â  setDirection(INITIAL_DIRECTION);
Â  Â  setFood({x: 15, y: 15});
Â  Â  setGameOver(false);
Â  Â  setWon(false);
Â  };

Â  return (
Â  Â  <div className="min-h-screen flex items-center justify-center px-4 animate-fade-in">
Â  Â  Â  <div className="text-center">
Â  Â  Â  Â  <h2 className="text-4xl font-bold text-white mb-4 animate-slide-left">
Â  Â  Â  Â  Â  {won ? 'ðŸŽ‰ You Win!' : gameOver ? 'Game Over!' : 'Snake Challenge'}
Â  Â  Â  Â  </h2>
Â  Â  Â  Â  <p className="text-xl text-gray-300 mb-6 animate-slide-right">
Â  Â  Â  Â  Â  {won ? 'You reached length 10!' : gameOver ? 'Try again!' : `Reach length ${TARGET_LENGTH} to continue (Current: ${snake.length})`}
Â  Â  Â  Â  </p>
Â  Â  Â  Â Â 
Â  Â  Â  Â  <div className="bg-white/10 backdrop-blur-lg rounded-2xl p-6 border border-white/10 inline-block mb-6 animate-scale-in">
Â  Â  Â  Â  Â  <divÂ 
Â  Â  Â  Â  Â  Â  style={{
Â  Â  Â  Â  Â  Â  Â  display: 'grid',
Â  Â  Â  Â  Â  Â  Â  gridTemplateColumns: `repeat(${GRID_SIZE}, ${CELL_SIZE}px)`,
Â  Â  Â  Â  Â  Â  Â  gap: '1px',
Â  Â  Â  Â  Â  Â  Â  backgroundColor: '#1a1a2e'
Â  Â  Â  Â  Â  Â  }}
Â  Â  Â  Â  Â  >
Â  Â  Â  Â  Â  Â  {Array.from({length: GRID_SIZE * GRID_SIZE}).map((_, idx) => {
Â  Â  Â  Â  Â  Â  Â  const x = idx % GRID_SIZE;
Â  Â  Â  Â  Â  Â  Â  const y = Math.floor(idx / GRID_SIZE);
Â  Â  Â  Â  Â  Â  Â  const isSnake = snake.some(segment => segment.x === x && segment.y === y);
Â  Â  Â  Â  Â  Â  Â  const isHead = snake[0].x === x && snake[0].y === y;
Â  Â  Â  Â  Â  Â  Â  const isFood = food.x === x && food.y === y;

Â  Â  Â  Â  Â  Â  Â  return (
Â  Â  Â  Â  Â  Â  Â  Â  <div
Â  Â  Â  Â  Â  Â  Â  Â  Â  key={idx}
Â  Â  Â  Â  Â  Â  Â  Â  Â  style={{
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  width: CELL_SIZE,
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  height: CELL_SIZE,
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  backgroundColor: isFood ? '#ef4444' : isSnake ? (isHead ? '#a855f7' : '#8b5cf6') : '#0f172a',
Â  Â  Â  Â  Â  Â  Â  Â  Â  Â  transition: 'background-color 0.1s ease'
Â  Â  Â  Â  Â  Â  Â  Â  Â  }}
Â  Â  Â  Â  Â  Â  Â  Â  />
Â  Â  Â  Â  Â  Â  Â  );
Â  Â  Â  Â  Â  Â  })}
Â  Â  Â  Â  Â  </div>
Â  Â  Â  Â  </div>

Â  Â  Â  Â  <div className="space-y-4">
Â  Â  Â  Â  Â  <p className="text-gray-400 animate-fade-in">Use arrow keys to move</p>
Â  Â  Â  Â  Â  {won ? (
Â  Â  Â  Â  Â  Â  <button
Â  Â  Â  Â  Â  Â  Â  onClick={handleWinContinue}
Â  Â  Â  Â  Â  Â  Â  className="bg-gradient-to-r from-purple-500 to-pink-500 text-white px-8 py-4 rounded-full font-semibold hover:shadow-lg hover:shadow-purple-500/50 transition-all transform hover:scale-105 animate-scale-in"
Â  Â  Â  Â  Â  Â  >
Â  Â  Â  Â  Â  Â  Â  Continue to Search <ChevronRight className="inline ml-2" size={20} />
Â  Â  Â  Â  Â  Â  </button>
Â  Â  Â  Â  Â  ) : (
Â  Â  Â  Â  Â  Â  <div className="space-x-4 animate-fade-in">
Â  Â  Â  Â  Â  Â  Â  {gameOver && (
Â  Â  Â  Â  Â  Â  Â  Â  <button
Â  Â  Â  Â  Â  Â  Â  Â  Â  onClick={handleRestart}
Â  Â  Â  Â  Â  Â  Â  Â  Â  className="bg-gradient-to-r from-purple-500 to-pink-500 text-white px-8 py-4 rounded-full font-semibold hover:shadow-lg hover:shadow-purple-500/50 transition-all transform hover:scale-105"
Â  Â  Â  Â  Â  Â  Â  Â  >
Â  Â  Â  Â  Â  Â  Â  Â  Â  Restart
Â  Â  Â  Â  Â  Â  Â  Â  </button>
Â  Â  Â  Â  Â  Â  Â  )}
Â  Â  Â  Â  Â  Â  Â  <button
Â  Â  Â  Â  Â  Â  Â  Â  onClick={() => setShowSnakeGame(false)}
Â  Â  Â  Â  Â  Â  Â  Â  className="bg-white/10 text-white px-8 py-4 rounded-full font-semibold hover:bg-white/20 transition-all transform hover:scale-105"
Â  Â  Â  Â  Â  Â  Â  >
Â  Â  Â  Â  Â  Â  Â  Â  Back to Home
